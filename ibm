  image: ibmcom/db2
  environment:
    DB2INSTANCE: db2inst1
    DB2INST1_PASSWORD: password
    LICENSE: accept
    DBNAME: nPolice
  privileged: true
  #volumes:
  #  - ./database:/database 
  networks:
    - backend
