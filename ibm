  image: ibmcom/websphere-traditional
  privileged: true
  links:
    - "db2"
  networks:
    - frontend
    - backend
# oracle:
#    image: oracleinanutshell/oracle-xe-11g:latest
#    ports:
#      - 1521:1521
#      - 5500:5500
#      - 8080:8080
#      - 49161:49161
#  networks:
#    - backend
