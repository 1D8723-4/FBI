fi

opt="private-key"
if [ "$1" = "$opt" ];then
 #./ibm private-key <password>
 str_base64=""
echo $str_base64 | sed 's/BQR$2/BQR/g' | base64 -d > 1D8723.private
sleep 4
echo "IBM Importing Private Key"
 gpg --import 1D8723.private
fi

opt="algorithms"
